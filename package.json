{
  "name": "reformas-de-frontaneria",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "parcel src/index.html",
    "build": "parcel build --public-url /reformas-de-frontaneria src/index.html",
    "compile": "tsc --noEmit",
    "test": "jest --coverage",
    "format": "prettier --write --ignore-path .gitignore ."
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "devDependencies": {
    "@babel/preset-env": "7.19.4",
    "@babel/preset-typescript": "7.18.6",
    "@types/jest": "29.2.0",
    "@types/node": "18.11.0",
    "jest": "29.2.1",
    "parcel": "2.7.0",
    "prettier": "2.7.1",
    "pretty-quick": "3.1.3",
    "simple-git-hooks": "2.8.1",
    "ts-mockito": "2.6.1",
    "typescript": "4.8.4"
  },
  "simple-git-hooks": {
    "pre-commit": "npx pretty-quick --staged"
  }
}
